rv-wrapper",t):this.$el.on("mousedown.revolver",".rv-wrapper",u).on("dragstart.revolver","img",function(t){return t.preventDefault()})},getNewPosition:function(){return this.improveClosest(),this.currentSlide>this.lastSlide?this.currentSlide=this.lastSlide:this.newX>=0&&(this.currentSlide=0),this.currentSlide},improveClosest:function(){var t,i,e,n,s,r,o;for(s=this.positions,o=[],i=e=0,n=s.length;n>e;i=++e)t=s[i],o.push(t>(r=this.newX+this.itemWidth/20)&&r>this.positions[i+1]?this.currentSlide="next"===this.moveDirection()?i:i+1:void 0);return o},moveDirection:function(){return this.playDirection=this.newRelativeX>0?"next":"prev",this.playDirection},bindCustomEvents:function(){return this.$el.on("next.revolver",function(t){return function(){return t.next()}}(this)).on("prev.revolver",function(t){return function(){return t.prev()}}(this)).on("play.revolver",function(t){return function(){return t.play()}}(this)).on("stop.revolver",function(t){return function(){return t.stop()}}(this))},bindStopOnHover:function(){return this.opts.stopOnHover&&this.opts.autoplayMS>0&&!o?(this.$el.on("mouseover.revolver",function(t){return function(){return t.stop()}}(this)),this.$el.on("mouseout.revolver",function(t){return function(){return t.play()}}(this))):void 0},destroy:function(){return e(i).off(".revolver"),this.destroyControls(),this.$el.off(".revolver").removeData("revolver")}},e.fn.revolver=function(t){return this.each(function(){return e(this).data("revolver")?void 0:e(this).data("revolver",new n(t,this))})}}(window,document,window.jQuery||window.Zepto);

var initHeadroom = function($, wrapperClass, options){
  var main = $(wrapperClass)[0];
  if (main) {
    // var hamburgerLeft = $(".main-menu-toggle.left-panel .hamburger");
    // var wrapperLeft = $(".side-panel-wrapper.left");
    var headroom = new Headroom(
      main,
      $.extend({
        tolerance: 5,
        offset: 100,
        classes: {
          initial: "animated",
          pinned: "slideDown",
          unpinned: "slideUp"
        },
      }, options || {})
    );
    headroom.init();
    $(main).data("headroom-instance", headroom);
  }
};

var initBurgerNav = function($, wrapperSelector, triggerSelector){
  var $trigger = $(triggerSelector);
  var $wrapper = $(wrapperSelector);
  if ($trigger.length>0 && $wrapper.length>0) {
    $trigger.click(function(){
      if ($wrapper.hasClass('burger-opened')) {
        $wrapper.removeClass('burger-opened');
      } else {
        $wrapper.addClass('burger-opened');
      }
    });
  }
}

var initShare = function($, wrapperSelector, triggerSelector, popupSelector){
  var $wrapper = $(wrapperSelector);
  if ($wrapper.length>0) {
    var $trigger = $wrapper.find(triggerSelector);
    var $popup = $wrapper.find(popupSelector);
    if ($trigger.length>0 && $popup.length>0){
      $trigger.click(function(){
        if ($popup.hasClass('opened')) {
          $popup.removeClass('opened');
        } else {
          $popup.addClass('opened');
          $(window).one('scroll', function(){
            $popup.removeClass('opened');
          });
        }
        return false;
      });
    }
  }
}

var initSlides = function($, wrapperSelector, containerSelector, items){
  var $wrappers = $(wrapperSelector);
  if ($wrappers.length>0){
    $wrappers.forEach(function(wrapper){
      var $wrapper = $(wrapper);
      var $prev = $wrapper.find('.prev');
      var $next = $wrapper.find('.next');
      var $container = $wrapper.find(containerSelector);
      $container.revolver({
        autoplayMS: false,
        navigation: false,
        // pagination: false,
        items: items,
        breakpoints: [
          [768, 1],
        ],
      });
      var revolver = $container.data('revolver');
      $prev.click(function(){
        revolver.prev();
        return false;
      });
      $next.click(function(){
        revolver.next();
        return false;
      });
    })
  }
};

var initContactForm = function($, wrapperSelector, triggerSelector){
  var $wrapper = $(wrapperSelector);
  var $triggers = $(triggerSelector);
  if ($wrapper.length>0 && $triggers.length>0){
    $triggers.click(function(){
      if ($wrapper.hasClass('opened')){
        $wrapper.removeClass('opened');
      } else {
        $wrapper.addClass('opened');
        $wrapper.find('.close').one('click', function(){
          $wrapper.removeClass('opened');
        });
      }
    })
  }
};

Zepto(function($){
  initHeadroom($, '.main-menu');
  initBurgerNav($, '.main-menu', '.burger-nav');
  initShare($, '.share', '.trigger', '.share-popup');
  initSlides($, '.section-properties', '.carousel', 2);
  initSlides($, '.section-communities', '.carousel', 4);
  initSlides($, '.section-property-fullscreen-gallery', '.carousel', 1);
  initContactForm($, '.contact-form', '[data-contact-form-trigger="1"]');
});
